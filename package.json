  "bugs": {
    "url": "https://github.com/SumnanAzadi/react_ecommerce/issues"
  },
  "homepage": "https://github.com/SumnanAzadi/react_ecommerce#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.9",
    "multer": "^1.4.2",
    "path": "^0.12.7"
  }
}
